<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="alfakhamah_assets" inherit_id="theme_laze.assent_front" name="alfakhamah_assets">
        <xpath expr="." position="inside">
            <link href="/alfakhamah_store/static/src/css/fonts.css" rel="stylesheet"/>
        </xpath>
        
        <xpath expr="link[last()]" position="after">
            <link href="/alfakhamah_store/static/src/css/custom.css" rel="stylesheet"/>
            <link href="/alfakhamah_store/static/src/less/colors.less" rel="stylesheet" type="text/less"/>
            <link href="/alfakhamah_store/static/src/less/custom.less" rel="stylesheet" type="text/less"/>
            <link t-if="lang_direction == 'rtl'" href="/alfakhamah_store/static/src/css/rtl.css" rel="stylesheet"/>
        </xpath>
    </template>

    <template id="alfakhamah_product_slider_assets" inherit_id="website.assets_frontend" priority="18">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/alfakhamah_store/static/src/js/theme_front.js"/> 
        </xpath>
    </template>

</odoo>
